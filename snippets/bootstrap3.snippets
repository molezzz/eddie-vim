		<li class="span4">
			<a href="${1:#}" class="thumbnail">
				<img src="${2:image.jpg}" alt="">
			</a>
		</li>
		${3:...}
	</ul>


snippet b3-thumbnailsdiv
	<ul class="thumbnails">
		<li class="span4">
			<div class="thumbnail">
				<img src="${1:image.jpg}" alt="">
				<h3>${2:Thumbnail label}</h3>
				<p>${3:Thumbnail caption...}</p>
			</div>
		</li>
		${4:...}
	</ul>

snippet b3-uneditableinput
	<span class="input-large uneditable-input">${1:Some value here}</span>

snippet b3-unstyled
	<ul class="unstyled">
		<li>${1:...}</li>
	</ul>

snippet b3-well
	<div class="well">
		${1:...}
	</div>

snippet b3-welllarge
	<div class="well well-large">
		${1:...}
	</div>

snippet b3-wellsmall
	<div class="well well-small">
		${1:...}
	</div>